rofile-auth-profile\" style={{display: menuState}}>\n            <div className=\"settings-inner-container\">\n                <div className=\"divider card-header\">\n                    <h1 id='card-header-title'>Instelligen van vragenlijst</h1>\n                    {questionnaires && questionnaires.map(questionnaire => (\n                        <div key={questionnaire.ID}>\n                            <h2>{questionnaire.Title}</h2>\n                        </div>\n                     ))}\n                </div>\n                <div className='divider'>\n                    {questionnaires && questionnaires.map(questionnaire => (\n                        <QuestionnaireField questionnaire={questionnaire && questionnaire}/>\n                    ))}\n                </div>\n                <div id='print-questionnaire-container'>\n                    <img src={printIcon} alt=\"\" onClick={printQuestionnaire} />\n                    <p onClick={printQuestionnaire}>Print vragenlijst</p>\n                </div>\n                <div>\n                    <p>Vragenlijst is in te vullen op</p>\n                    {questionnaires && questionnaires.map(questionnaire => (\n                    <a href='https://deccos.nl/Questionnaires/${questionnaire.ID}'>https://deccos.nl/Questionnaires/{questionnaire.ID}</a>\n                    ))}\n                </div>\n            </div>\n        </div>\n        <RightSideBar />\n    </div>\n  )\n}\n\nexport default QuestionnaireSettingsDetail","/Volumes/GoogleDrive/Mijn Drive/Deccos/deccos-webapp/src/components/ProjectManagement/AddMilestone.jsx",["2227","2228","2229","2230"],"/Volumes/GoogleDrive/Mijn Drive/Deccos/deccos-webapp/src/components/ProjectManagement/MilestoneSettings.jsx",["2231","2232","2233"],"import RightSideBar from \"../rightSideBar/RightSideBar\"\nimport LeftSideBar from \"../LeftSideBar\";\nimport LeftSideBarFullScreen from \"../LeftSideBarFullScreen\";\nimport MenuStatus from \"../../hooks/MenuStatus\";\nimport