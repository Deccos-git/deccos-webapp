       setOnline(false)\n      }\n    })\n  }, [])\n\n    const AuthRedirect = () => {\n      if(online === false && client != 'NewClient' && client != '' ){\n        return <LoginRegister/>\n      } else if (online === true && approved === true && compagny.includes(client) && client != '') {\n        \n        return ( \n        <AuthProvider>\n        <MenuProvider>\n        <ColorProvider>\n          <>\n          <Topbar />\n          <Main/>\n          <BottomBar/>\n          <Footer/>\n          </>\n        </ColorProvider>\n        </MenuProvider>\n        </AuthProvider>\n        )\n      } else if (online === true && approved === false && compagny.includes(client) && client != ''){\n        return <NotApproved/>\n      } else if (online === true && client === '' ){\n        return (\n        <AuthProvider>\n          <MultipleAccounts />\n        </AuthProvider>\n        )\n      } else if (client === 'NewClient' && online === false) {\n        return (\n          <>\n            <Main/>\n            <Footer/>\n          </>\n        )\n      } else if (client === 'NewClient' && online === true) {\n        return (\n          <AuthProvider>\n            <MultipleAccounts />\n          </AuthProvider>\n          )\n      }else if(online === false && client != 'NewClient' && client === '' ){\n        return <LoginRegister/>\n      } else {\n        return null\n      }\n    }\n\n    const updateUserOnlineInStatus = (status) => {\n      auth.onAuthStateChanged(User =>{\n        if(User){\n          db.collection(\"Users\")\n          .doc(User.uid)\n          .update({\n            Online: status\n          })\n        }\n      })\n    }\n  \n    const checkOnlineStatus = () => {\n        if(navigator.onLine){\n          updateUserOnlineInStatus(true)\n        } else if (!navigator.onLine) {\n          updateUserOnlineInStatus(false)\n        }\n    }\n\n    checkOnlineStatus()\n\n  const variants = {\n    hidden: { opacity: 0 },\n    visible: { opacity: 1 },