\n            const result = range(start, end)\n              \n            setRange(result)\n        }\n\n        }, [field])\n\n        if(field.Type === 'paragraph'){\n            return (\n                <div className='question-container' key={field.ID}>\n                    <h2>{field.Question}</h2>\n                    <textarea name=\"\" id=\"\" cols=\"30\" rows=\"10\" placeholder='Geef hier uw antwoord'></textarea>\n                </div>\n            )\n            } else if(field.Type === 'scale'){\n                return (\n                    <div className='question-container' key={field.ID}>\n                        <h2>{field.Question}</h2>\n                        <div id='scale-container'>\n                            {field.ReachStartLable}\n                            {range && range.map(btn => (\n                                <div id='question-type-label-container'>\n                                        <input type=\"radio\" value={btn} name={field.Question} />\n                                        <label htmlFor={btn}>{btn}</label>\n                                </div>\n                            ))}\n                            {field.ReachEndLabel}\n                        </div>\n                    </div>\n                )\n            } else {\n                return null\n        }\n    }\n\n    const printQuestionnaire = () => {\n        window.print()\n    }\n\n\n  return (\n     <div className=\"main\">\n        <LeftSideBarAuthProfile />\n        <LeftSideBarAuthProfileFullScreen/>\n        <div className=\"profile profile-auth-profile\" style={{display: menuState}}>\n            <div className=\"settings-inner-container\">\n                <div className=\"divider card-header\">\n                    <h1 id='card-header-title'>Instelligen van vragenlijst</h1>\n                    {questionnaires && questionnaires.map(questionnaire => (\n                        <div key={questionnaire.ID}>\n                            <h2>{questionnaire.Title}</h2>\n                        </div>\n                     ))}\n                </div>\n                <div className='divider'>\n                    {questionnaires && questionnaires.map(questionnaire => (\n                        <QuestionnaireField questionnaire={questionnaire && questionnaire}/>\n                    ))}\n                </div>\n                <div id='print-questionnaire-container'>\n                    <img src={printIcon} alt=\"\" onClick={printQuestionnaire} />\n                    <p onClick={printQuestionnaire}>Print vragenlijst</p>\n                </div>\n                <div>\n                    <p>Vragenlijst is in te vullen op</p>\n                    {questionnaires && questionnaires.map(questionnaire => (\n                    <a href='https://deccos.nl/Questionnaires/${questionnaire.ID}'>https://deccos.nl/Questionnaires/{questionnaire.ID}</a>\n                    ))}\n                </div>\n            </div>\n        </div>\n        <RightSideBar />\n    </div>\n  )\n}\n\nexport default QuestionnaireSettingsDetail","/Volumes/GoogleDrive/Mijn Drive/Deccos/deccos-webapp/src/components/ProjectManagement/AddMilestone.jsx",["2643","2644","2645","2646"],"import LeftSideBar from \"../LeftSideBar\"\nimport LeftSideBarFullScreen from \"../LeftSideBarFullScreen\"\nimport MenuStatus from \"../../hooks/MenuStatus\";\nimport {useFirestore, useFirestoreActivities} from \"../../firebase/useFirestore\"\nimport { useState, useEffect, useContext } from 'react'\nimport { Auth } from '../../StateManagment/Auth';\nimport uuid from 'react-uuid';\nimport { db, timestamp } from \"../../firebase/config.js\"\nimport { client } from \"../../hooks/Client\"\nimport deleteIcon from '../../images/icons/delete-icon.png'\nimport { useHistory } from \"react-router-dom\";\nimport arrowLeft from '../../images/icons/arrow-left-icon.png'\nimport arrowRight from '../../images/icons/arrow-right-icon.png'\nimport capIcon from '../../images/icons/c