estore('Colors')\n\n    useEffect(() => {\n        colors && colors.forEach(color => {\n            const background = color.Background \n\n            setColor(background)\n        })\n\n    },[colors])\n\n    const Goals = () => {\n        const goals = useFirestore('Goals')\n\n        return(\n            <>\n            {goals && goals.map(goal => (\n            <div>\n                <h1>Doel</h1>\n            <img id='impact-dasboard-goal-banner' src={goal.Banner} alt=\"\" />\n            <div id='impact-progress-goal-container' className='divider'>\n                <h2>{goal.Title}</h2>\n                <div id='goal-meta-container'>\n                    <div className='goal-meta-inner-container'>\n                        <div className='goal-meta-title-container'>\n                            <img src={worldIcon} alt=\"\" />\n                            <h3>SDGs</h3>\n                        </div>\n                        <SDGS goal={goal}/>\n                    </div>\n                    <div className='goal-meta-inner-container'>\n                        <div className='goal-meta-title-container'>\n                            <img src={groupIcon} alt=\"\" />\n                            <h3>Doelgroep</h3>\n                        </div>\n                        <p>{goal.Targetgroup}</p>\n                    </div>\n                    <div className='goal-meta-inner-container'>\n                        <div className='goal-meta-title-container'>\n                            <img src={newUserIcon} alt=\"\" />\n                            <h3>Impact op doelgroep</h3>\n                        </div>\n                        <p>{goal.ImpactTargetgroup}</p>\n                    </div>\n                    <div className='goal-meta-inner-container'>\n                        <div className='goal-meta-title-container'>\n                            <img src={leafIcon} alt=\"\" />\n                            <h3>Impact maatschappij</h3>\n                        </div>\n                        <p>{goal.ImpactSociety}</p>\n                    </div>\n                    <div className='goal-meta-inner-container'>\n                        <div className='goal-meta-title-container'>\n                            <img src={preconditionsIcon} alt=\"\" />\n                            <h3>Randvoorwaarden</h3>\n                        </div>\n                        <p>{goal.Preconditions}</p>\n                    </div>\n                    <div className='goal-meta-inner-container'>\n                        <div className='goal-meta-title-container'>\n                            <img src={externalFactorsIcon} alt=\"\" />\n                            <h3>Externe factoren</h3>\n                        </div>\n                        <p>{goal.ExternalFactors}</p>\n                    </div>\n                </div>\n            </div>\n            <Activities goal={goal}/>\n            </div>\n            ))}\n        </>\n        )\n    }\n\n    const SDGS = ({goal}) => {\n        const SDGs = useFirestoreSDGsSelected(goal.ID)\n\n        return(\n            <ul>\n                {SDGs && SDGs.map(sdg => (\n                   <li>{sdg.SDG}</li>\n                ))}\n            </ul>\n        )\n    }\n\n    const ProgressionBarGoal = ({goal}) => {\n\n        const tasks = useFirestoreTasksGoals(goal.ID)\n        const tasksCompleted = useFirestoreTasksCompleteGoals(goal.ID)\n\n        const completedArray = []\n        const totalArray = []\n\n        tasks && tasks.forEach(task => {\n            totalArray.push(task)\n        })\n\n        tasksCompleted && tasksCompleted.forEach(task => {\n            completedArray.push(task)\n        })\n\n        const onePercentage = totalArray.length !== 0 ? totalArray.length/100 : 0\n        const completed = completedArray.length !== 0 ? completedArray.length : 1\n\n        const average = onePercentage*completed\n\n        return(\n            <div className='progressionbar-completed' style={{width: `${average}%`}}></div>\n        )\n    }\n\n    const Activities = ({goal}) => {\n\n        const activities = useFirestoreActivities(goal.ID)\n\n        return(\n            <>\n                <h2>Activiteiten</h2>\n                <div id='activity-outer-container'>\n                {activities && activities.map(activity => (\n                    <div className='activity-inner-container-dashboard' key={activity.ID} style={{backgroundColor: color}}>\n                        <img id='impact-dasboard-activity-banner' src={activity.Banner} alt=\"\" />\n                        <h3 id='activity-title'>{activity.Activity}</h3>\n                        <div className='goal-meta-inner-container'>\n                            <div className='goal-meta-title-container'>\n                                <img src={impactIcon} alt=\"\" />\n                                <h3>Impact</h3>\n                            </div>\n                            <p>{activity.Impact}</p>\n                        </div>\n                        <div className='goal-meta-title-container'>\n                                <img src={progressIcon} alt=\"\" />\n                                <h3>Voortgang</h3>\n                            </div>\n                            <ProgressionBarActivity activity={activity}/>\n                        <div>\n                            <Outputs activity={activity}/>\n                        </div>\n                        {/* <div className='progressionbar-outer-bar'>\n                            <ProgressionBarActivity activity={activity}/>\n                            <div className='progressionbar-completed' style={{width: `${activity.Progress}%`}}></div>\n                        </div> */}\n                    </div>\n                ))}\n                </div>\n            </>\n        )\n    }\n\n    const Outputs = ({activity}) => {\n        const outputs = useFirestoreOutputs(activity.ID)\n\n        return(\n            <div>\n                <div className='activity-meta-title-container'>\n                    <img src={outputIcon} alt=\"\" />\n                    <h3>Outputs</h3>\n                </div>\n                {outputs && outputs.map(output => (\n                    <div className='impact-dashboard-output-container'>\n                        <h3 className='output-title'>{output.Title}</h3>\n                        <div className='dashboard-instruments-container'>\n                            <div className='activity-meta-title-container'>\n                                <img src={effectIcon} alt=\"\" />\n                                <h4>Effect</h4>\n                            </div>\n                            <Effects output={output}/>\n                        </div>\n                        <div className='dashboard-instruments-container'>\n                            <div className='activity-meta-title-container'>\n                                <img src={sroiIcon} alt=\"\" />\n                                <h4>SROI</h4>\n                            </div>\n                            <p className='output-seeting-effect'><SROI output={output}/></p>\n                        </div>\n                        <div className='dashboard-instruments-container'>\n                            <Instruments output={output}/>\n                        </div>\n                        <Milestones output={output}/>\n                    </div>\n                ))}\n                \n            </div>\n        )\n    }\n\n    const Effects = ({output}) => {\n\n        const effects = useFirestoreOutputEffects(output.ID)\n\n        return(\n            <ul>\n            {effects && effects.map(effect => (\n                <li>{effect.Effect}</li>\n            ))}\n            </ul>\n        )\n    }\n\n    const SROI = ({output}) => {\n\n        const SROIs = useFirestoreSROIs(output.ID)\n\n        return(\n            <>\n                {SROIs && SROIs.map(SROI => (\n                    <div>\n                        <p><b>{SROI.Title}</b></p>\n                        <p>{SROI.Value} per {SROI.Output.toLowerCase()}</p>\n                    </div>\n                ))}\n            </>  \n        )\n    }\n\n    const Milestones = ({output}) => {\n\n        const milestones = useFirestoreMilestones(output.ID) \n\n        return(\n            <div className='dashboard-instruments-container'>\n                <div className='activity-meta-title-container'>\n                    <img src={festiveIcon} alt=\"\" />\n                    <h4>Mijlpalen</h4>\n                </div>\n                {milestones && milestones.map(milestone => (\n                    <div className='impact-dashboard-output-container' style={{backgroundColor: color}}>\n                        <h5>{milestone.Title}</h5>\n                        <MilestoneProgress milestone={milestone}/>\n                    </div>\n                ))}\n            </div>\n        )\n    }\n\n    const Instruments = ({output}) => {\n        const instruments = useFirestoreImpactInstruments(output.ID) \n\n        const SROI = output.SROI\n\n        const datatype = (instrument) => {\n\n            if(instrument.Type === 'Manual'){\n                return \"Handmatig\"\n            } else if(instrument.Type === 'Questionnairy'){\n                return 'Vragenlijst'\n            }\n        }\n        \n        return(\n            <div className='output-instrument-inner-container'>\n                <div className='activity-meta-title-container'>\n                    <img src={measureIcon} alt=\"\" />\n                    <h4>Meetinstrumenten</h4>\n                </div>\n                <div className='output-seeting-effect'>\n                    {instruments && instruments.map(instrument => (\n                        <div className='dashboard-internal-results-container' style={{backgroundColor: color}}>\n                            <p data-id={instrument.ID}>{instrument.Title}</p>\n                            <div className='activity-meta-title-container'>\n                                <img src={typeIcon} alt=\"\" />\n                                <h5>Type</h5>\n                            </div>\n                            <p className='output-seeting-effect'>{datatype(instrument)}</p>\n                            <Results instrument={instrument} SROI={SROI}/>\n                        </div>\n                    ))}\n                </div>\n            </div>\n        )\n    }\n\n   const MilestoneProgress = ({milestone}) => {\n       \n        const results = useFirestoreResults(milestone.InstrumentID)\n\n        const goal = milestone.Number\n\n        const width = results.length*100/goal\n\n        const percentage = `${width}%`\n\n        const checkSucces = () => {\n            db.collection('Milestones')\n            .doc(milestone.docid)\n            .update({\n                Succes: true\n            })\n        }\n\n        const succes = () => {\n\n            if(results.length >= goal){\n                checkSucces()\n                return '#00cd00'\n            } else {\n                return '#63cadc'\n            }\n        }\n\n    return(\n        <div className='milestone-progress-container'>\n            <div className='percentage-container'>\n                <p>Huidig: {results.length} ({width}%)</p>\n                <p>Doel: {goal}</p>\n            </div>\n            \n            <div className='progressbar-outer-bar'>\n                <div className='progressbar-progress' style={{width: percentage, backgroundColor: succes()}}></div>\n            </div>\n\n        </div>\n        \n        )\n   }\n\n    const ProgressionBarActivity = ({activity}) => {\n\n        const tasks = useFirestoreTasksActivities(activity.ID)\n        const tasksCompleted = useFirestoreTasksCompleteActivities(activity.ID)\n\n        const completedArray = []\n        const totalArray = []\n\n        tasks && tasks.forEach(task => {\n            totalArray.push(task)\n        })\n\n        tasksCompleted && tasksCompleted.forEach(task => {\n            completedArray.push(task)\n        })\n\n        const task = () => {\n            if(completedArray.length != 1){\n                return 'taken'\n            } else {\n                return 'taak'\n            }\n        }\n\n        return(\n            <div>\n                <p className='output-seeting-effect'>{completedArray.length} {task()} afgerond van de {totalArray.length} taken</p>\n            </div>\n        )\n    }\n\n    const Results = ({instrument, SROI}) => {\n\n        const dataset = useFirestoreResults(instrument.ID)\n        const totalSROI = dataset.length*SROI\n\n      if(instrument.Type === 'Manual'){\n\n            return(\n                <>\n                <div className='internal-results-container'>\n                    <div className='activity-meta-title-container'>\n                        <img src={resultsIcon} alt=\"\" />\n                        <h5>Resultaat</h5>\n                    </div>\n                    <ManualResultsGraph instrument={instrument}/>\n                </div>\n                <div className='internal-results-container'>\n                    <div className='activity-meta-title-container'>\n                        <img src={resultsIcon} alt=\"\" />\n                        <h5>Totale SROI</h5>\n                    </div>\n                    <p className='output-seeting-effect'>{dataset.length} x {SROI} = {totalSROI}</p>\n                </div>\n                </>\n            )\n        } else if(instrument.Type === 'Questionnairy'){\n\n            return(\n                <div className='internal-results-container'>\n                    <div className='activity-meta-title-container'>\n                        <img src={resultsIcon} alt=\"\" />\n                        <h5>Resultaat</h5>\n                    </div>\n                    <QuestionnaireResults instrument={instrument}/>\n                </div>\n            )\n        }\n         else {\n            return(\n                <></>\n            )\n        }\n    }\n\n    const QuestionnaireResults = ({instrument}) => {\n\n        const questionnaires = useFirestoreID('Questionnaires', instrument.Output.ID) \n\n        return(\n            <div>\n                {questionnaires && questionnaires.map(questionnaire => (\n                    <div className='questionnaire-results-container'>\n                        <p>Aantal responses</p>\n                        <p>{questionnaire.Responses ? questionnaire.Responses : 0}</p>\n                        <p>Bekijk analyse</p>\n                    </div>\n                ))}\n            </div>\n        )\n    }\n\n    return (\n        <div className=\"main\">\n            <LeftSideBar />\n            <LeftSideBarFullScreen/>\n            <div className=\"main-container\" style={{display: menuState}}>\n                <div className=\"page-header\">\n                    <h1>Impact dashboard</h1>  \n                </div>\n                <div className='profile profile-auth-profile'>\n                    <Goals/>\n                </div>   \n            </div>\n        </div>\n    )\n}\n\nexport default ImpactProgress\n","/Volumes/GoogleDrive/Mijn Drive/Deccos/deccos-webapp/src/components/Impact/Stakeholders.jsx",["1643","1644"],"import {useFirestore} from \"../../firebase/useFirestore\"\nimport LeftSideBar from \"../LeftSideBar\"\nimport LeftSideBarFullScreen from \"../LeftSideBarFullScreen\"\nimport MenuStatus from \"../../hooks/MenuStatus\";\nimport arrowUpIcon from '../../images/icons/arrow-up-icon-white.png'\nimport penIcon from '../../images/icons/pen-icon.png'\nimport { NavLink, Link } from \"react-router-dom\";\nimport { client } from '../../hooks/Client';\n\nconst Stakeholders = () => {\n    const menuState = MenuStatus()\n    const stakeholders = useFirestore('Stakeholders')\n\n    return (\n        <div className=\"main\">\n            <LeftSideBar />\n            <LeftSideBarFullScreen/>\n            <div className=\"main-container\" style={{display: menuState}}>\n                <div className=\"page-header\">\n                    <h1>Stakeholders</h1>\n                    <div className='edit-icon-header-container'>\n                        <NavLink activeClassName='active' to={`/${client}/StakeholderAnalysis`}>\n                            <img src={penIcon} alt=\"\" />\n                        </NavLink>\n                    </div>\n                </div>\n                <div className=\"card-container\">\n                    <div className='list-container list-container-stakeholder-management-page'>\n                        <div className='list-top-row-container'>\n                            <p>CATEGORIE</p>\n                            <p>ORGANISATIE</p>\n                            <p>CONTACTPERSOON</p>\n                        </div>\n                        {stakeholders && stakeholders.map(stakeholder => (\n                            <div className='list-row-container'>\n                                <p>{stakeholder.Categorie}</p>\n                                <p>{stakeholder.Organisation} </p>\n                                <p>{stakeholder.Name}</p>\n                            </div>  \n                        ))}\n                    </div>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default Stakeholders\n","/Volumes/GoogleDrive/Mijn Drive/Deccos/deccos-webapp/src/components/Impact/QuestionnaireAnalysis.jsx",["1645"],"import LeftSideBarAuthProfile from \"../LeftSideBarAuthProfile\";\nimport LeftSideBarAuthProfileFullScreen from \"../LeftSideBarAuthProfileFullScreen\";\nimport RightSideBar from \"../rightSideBar/RightSideBar\"\nimport MenuStatus from \"../../hooks/MenuStatus\";\nimport { client } from \"../../hooks/Client\"\nimport { useHistory } from \"react-router-dom\";\nimport uuid from 'react-uuid';\nimport { db, timestamp } from \"../../firebase/config.js\"\nimport { useFirestoreID, useFirestoreQuestionnairesResponses, useFirestoreQuestionnaireFields, useFirestoreQuestionnaireAnalysis } from \"../../firebase/useFirestore\"\nimport { useState, useEffect } from 'react';\nimport Location from \"../../hooks/Location\"\nimport ButtonClicked from \"../../hooks/ButtonClicked\";\n\nconst QuestionnaireAnalysis = () => {\n    const [questionnaireTitle, setQuestionnaireTitle] = useState('')\n\n    const menuState = MenuStatus()\n    const history = useHistory()\n    const id = uuid()\n    const route = Location()[3]\n    const options = { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric' };\n\n    const questionnaires = useFirestoreID('Questionnaires', route)\n    const responses = useFirestoreQuestionnairesResponses(route)\n    const questionnaireFields = useFirestoreQuestionnaireFields(route)\n    const analysis = useFirestoreQuestionnaireAnalysis(route)\n\n    useEffect(() => {\n      questionnaires && questionnaires.forEach(questionnaire => {\n          const title = questionnaire.Title\n\n          setQuestionnaireTitle(title)\n      })\n    }, [questionnaires]);\n    \n\n    const linkRespons = (e) => {\n        const id = e.target.dataset.id \n\n        history.push(`/${client}/QuestionnairesRespons/${id}`)\n    }\n\n    const analysisArray = []\n\n    const QuestionnaireFields = ({field}) => {\n        const [analyseWord, setAnalyseWord] = useState('')\n\n        if(field.Type === 'paragraph'){\n            return (\n                <div className='question-analyse-container'>\n                    <h3>{field.Question}</h3>\n                    <p>Analyseer deze vraag op basis van een analysewoord</p>\n                    <input type=\"text\" placeholder='Type hier een analysewoord' data-id={field.ID} onChange={(e) => setAnalyseWord(e.target.value)}/>\n                    <Analysis field={field} word={analyseWord}/>\n                </div>\n            )\n        } else if (field.Type === 'scale'){\n            return(\n                <div className='question-analyse-container'>\n                    <h3>{field.Question}</h3>\n                    <p>De gemiddelde score is: </p>\n                    <ScaleScore id={field.ID} question={field.Question}/>\n                </div>\n            )\n        }\n        else {\n            return null\n        }\n    }\n\n    const ScaleScore = ({id, question}) => {\n        const scoreArray = []\n\n        responses && responses.forEach(respons => {\n            respons.Input.forEach(input => {\n                if(input.ID === id){\n                    scoreArray.push(parseInt(input.Input))\n                }\n            })\n        })\n\n        const reducer = (previousValue, currentValue) => previousValue + currentValue;\n\n        const sum = scoreArray.reduce(reducer)\n\n        const average = sum/scoreArray.length\n\n         // Set to analysis rapport\n        const analysisObject = {\n            Question: question,\n            QuestionID: id,\n            Average: average\n        }\n\n        analysisArray.push(analysisObject)\n\n         return <p>{average}</p>\n    }\n\n    const Analysis = ({field, word}) => {\n        const [inputs, setInputs] = useState([])\n\n        const analyseButton = () => {\n            const inputArray = []\n            responses && responses.forEach(respons => {\n                respons.Input.forEach(input => {\n                    if(input.ID === field.ID){\n                        if(input.Input.includes(word)){\n                            inputArray.push(input)\n                        }\n                    }\n                })\n            })\n\n            // Set to analysis rapport\n            const analysisObject = {\n                Question: field.Question,\n                QuestionID: field.ID,\n                Word: word,\n                Count: inputArray.length\n            }\n\n            analysisArray.push(analysisObject)\n\n            setInputs(inputArray)\n        }\n\n        return (\n            <div>\n                <button className='button-simple' onClick={analyseButton}>Analyseer</button>\n                <p>Dit woord komt <b>{inputs.length}</b> keer voor in de responses op deze vraag</p>\n            </div>\n        )\n    }\n\n    const saveAnalyses = (e) => {\n\n        ButtonClicked(e, 'Opgeslagen')\n\n        db.collection('QuestionnaireAnalysis')\n        .doc()\n        .set({\n            Compagny: client,\n            Timestamp: timestamp,\n            QuestionnaireID: route,\n            QuestionnaireTitle: questionnaireTitle,\n            Analysis: analysisArray\n        })\n\n    }\n\n  return  <div className=\"main\">\n        <LeftSideBarAuthProfile />\n        <LeftSideBarAuthProfileFullScreen/>\n        <div className=\"profile profile-auth-profile\" style={{display: menuState}}>\n            <div className=\"settings-inner-container\">\n                <div className=\"divider card-header\">\n                    {questionnaires && questionnaires.map(questionnaire => (\n                        <div>\n                            <h1>Analyse van {questionnaire.Title}</h1>\n                            <p>Analyseer de responses van {questionnaire.Title}</p>\n                        </div>\n                    ))}\n                </div>\n                <div className='divider'>\n                    <h2>Analyseer responses</h2>\n                        {questionnaireFields && questionnaireFields.map(field => (\n                            <QuestionnaireFields field={field}/>\n                        ))}\n                        <div>\n                            <button onClick={saveAnalyses}>Analyse opslaan</button>\n                        </div>\n                </div>\n                <div className='divider'>\n                    <h2>{analysis && 'Analyses'}</h2>\n                    {analysis && analysis.map(analyse => (\n                        <div id='questionnaire-analysis-container'>\n                            {analyse.Analysis.map(question => (\n                                <div>\n                                    <h3>{question.Question}</h3>\n                                    <p>{question.Average && `Gemiddelde score ${question.Average}`}</p>\n                                    <p>{question.Word && `Analyse woord: ${question.Word}`}</p>\n                                    <p>{question.Count && `Aantal keer in responses: ${question.Count}`}</p>\n                                </div>\n                            ))}\n                        </div>\n                    ))}\n\n                </div>\n                <div className='divider'>\n                    <h2>Responses</h2>\n                    {responses && responses.map(respons => (\n                        <div id='respons-overview-container'>\n                            <p>{respons.Timestamp.toDate().toLocaleDateString(\"nl-NL\", options)}</p>\n                            <button className='button-simple' data-is={respons.ID} onClick={linkRespons}>Bekijk</button>\n                        </div>\n                    ))}\n                </div>\n            </div>\n        </div>\n        <RightSideBar />\n        </div>\n};\n\nexport default QuestionnaireAnalysis;\n","/Volumes/GoogleDrive/mijn drive/deccos/deccos-webapp/src/index.js",[],"/Volumes/GoogleDrive/mijn drive/deccos/deccos-webapp/src/App.js",["1646"],"/Volumes/GoogleDrive/mijn drive/deccos/deccos-webapp/src/reportWebVitals.js",[],"/Volumes/GoogleDrive/mijn drive/deccos/deccos-webapp/src/components/LoginRegister.js",["1647"],"/Volumes/GoogleDrive/mijn drive/deccos/deccos-webapp/src/components/topbar/Topbar.js",["1648","1649"],"/Volumes/GoogleDrive/mijn drive/deccos/deccos-webapp/src/firebase/config.js",[],"/Volumes/GoogleDrive/mijn drive/deccos/deccos-webapp/src/components/Main.js",[],"/Volumes/GoogleDrive/mijn drive/deccos/deccos-webapp/src/StateManagment/Auth.js",[],"/Volumes/GoogleDrive/mijn drive/deccos/deccos-webapp/src/Stat