               <ul>\n                    {assumptions && assumptions.map(assumption => (\n                        <li>{assumption.Assumption}</li>\n                    ))}\n                </ul>\n            </div>\n        )\n\n    }\n\n    const ExternalFactors = ({goal}) => {\n\n        const externalFactors = useFirestoreConditions(goal.ID)\n\n        return(\n            <div className='goal-meta-inner-container' style={{display: externalFactors.length > 0 ? 'block' : 'none'}}>\n                <div className='goal-meta-title-container'>\n                    <img src={externalFactorsIcon} alt=\"\" />\n                    <h3>Externe factoren</h3>\n                </div>\n                <ul>\n                    {externalFactors && externalFactors.map(factor => (\n                        <li>{factor.Condition}</li>\n                    ))}\n                </ul>\n            </div>\n        )\n\n    }\n\n    const SDGS = ({goal}) => {\n        const SDGs = useFirestoreSDGsSelected(goal.ID)\n\n        return(\n            <div className='goal-meta-inner-container' style={{display: SDGs.length > 0 ? 'block' : 'none'}}>\n                <div className='goal-meta-title-container'>\n                    <img src={worldIcon} alt=\"\" />\n                    <h3>SDGs</h3>\n                </div>\n                <ul>\n                    {SDGs && SDGs.map(sdg => (\n                    <li>{sdg.SDG}</li>\n                    ))}\n                </ul>\n            </div>\n        )\n    }\n\n    const Activities = ({goal}) => {\n\n        const activities = useFirestoreActivities(goal.ID)\n\n        return(\n            <div style={{display: activities.length > 0 ? 'block' : 'none'}}>\n                <h2>Activiteiten</h2>\n                <div id='activity-outer-container'>\n                {activities && activities.map(activity => (\n                    <div className='activity-inner-container-dashboard' key={activity.ID}>\n                        <img id='impact-dasboard-activity-banner' src={activity.Banner} alt=\"\" />\n                        <h3 id='activity-title'>{activity.Activity}</h3>\n                        <div className='goal-meta-inner-container' style={{display: activity.Impact ? 'block' : 'none'}}>\n                            <div className='goal-meta-title-container'>\n                                <img src={impactIcon} alt=\"\" />\n                                <h3>Impact</h3>\n                            </div>\n                            <p>{activity.Impact}</p>\n                            <Outputs activity={activity}/>\n                        </div>\n                    </div>\n                ))}\n                </div>\n            </div>\n        )\n    }\n\n    const Outputs = ({activity}) => {\n        const outputs = useFirestoreOutputs(activity.ID)\n\n        return(\n            <div style={{display: outputs.length > 0 ? 'block' : 'none'}}>\n                <div className='activity-meta-title-container'>\n                    <img src={outputIcon} alt=\"\" />\n                    <h3>Outputs</h3>\n                </div>\n                {outputs && outputs.map(output => (\n                    <div className='impact-dashboard-output-container'>\n                        <h3 className='output-title'>{output.Title}</h3>\n                        <Effects output={output}/>\n                        <div className='dashboard-instruments-container'>\n                            <ManualResults output={output}/>\n                        </div>\n                        <Research output={output}/>\n                        <Milestones output={output}/>\n                    </div>\n                ))} \n            </div>\n        )\n    }\n\n    const Effects = ({output}) => {\n\n        const effects = useFirestoreOutputEffects(output.ID)\n\n        return(\n            <div className='dashboard-instruments-container' style={{display: effects.length > 0 ? 'block' : 'none'}}>\n                <div className='activity-meta-title-container'>\n                    <img src={effectIcon} alt=\"\" />\n                    <h3>Effect</h3>\n                </div>\n                <ul>\n                {effects && effects.map(effect => (\n                    <li>{effect.Effect}</li>\n                ))}\n                </ul>\n            </div>\n        )\n    }\n\n    const Research = ({output}) => {\n\n        const researches = useFirestoreResearch(output.ID) \n\n        return(\n            <div className='dashboard-instruments-container' style={{display: researches.length > 0 ? 'block' : 'none'}}>\n                 <div className='activity-meta-title-container'>\n                    <img src={researchIcon} alt=\"\" />\n                    <h3>Onderzoeken</h3>\n                </div>\n                {researches && researches.map(research => (\n                    <>\n                        <div className='impact-dashboard-output-inner-container'>\n                            <h4>{research.Title}</h4>\n                            <MeasureMoments research={research}/>\n                            <Conclusions research={research}/>\n                        </div>\n                    </>\n                ))}\n            </div>\n        )\n    }\n\n    const MeasureMoments = ({research}) => {\n\n        const moments = useFirestoreMeasureMoments(research.ID)\n\n        return(\n            <div className='activity-meta-title-container' style={{display: moments.length > 0 ? 'block' : 'none'}}>\n                <div className='activity-meta-title-container'>\n                    <img src={calendarIcon} alt=\"\" />\n                    <h4>Meetmomenten</h4>\n                </div>\n                <div className='table-container output-seeting-effect'>\n                    <table className='table-impact-dashboard'>\n                        <tr>\n                            <th>MEETMOMENT</th>\n                            <th>DATUM</th>\n                        </tr>\n                        {moments && moments.map(moment => (\n                            <tr>\n                                <td>{moment.Title}</td>\n                                <td>{moment.Moment}</td>\n                            </tr>\n                        ))}\n                    </table>\n                </div>   \n            </div>\n        )\n\n    }\n\n    const Conclusions = ({research}) => {\n\n        const conclusions = useFirestoreConclusions(research.ID)\n\n        const conclusionType = (conclusion) => {\n            if(conclusion.Type == 'Plus'){\n                return 'Pluspunt'\n            } else if (conclusion.Type == 'Learningpoint'){\n                return 'Verbeterpunt'\n            }\n        }\n\n        return(\n            <div className='activity-meta-title-container' style={{display: conclusions.length > 0 ? 'block' : 'none'}}>\n                <div className='activity-meta-title-container'>\n                    <img src={checkIcon} alt=\"\" />\n                    <h4>Conclusies</h4>\n                </div>\n                <div className='table-container output-seeting-effect'>\n                    <table className='table-impact-dashboard'>\n                        <tr>\n                            <th>CONCLUSIE</th>\n                            <th>TYPE</th>\n                        </tr>\n                        {conclusions && conclusions.map(conclusion => (\n                            <tr>\n                                <td>{conclusion.Conclusion}</td>\n                                <td>{conclusionType(conclusion)}</td>\n                            </tr>\n                        ))}\n                    </table>\n                </div>   \n            </div>\n        )\n    }\n\n    const Milestones = ({output}) => {\n\n        const milestones = useFirestoreMilestones(output.ID) \n\n        return(\n            <div className='dashboard-instruments-container' style={{display: milestones.length > 0 ? 'block' : 'none'}}>\n                <div className='activity-meta-title-container'>\n                    <img src={growIcon} alt=\"\" />\n                    <h3>Mijlpalen</h3>\n                </div>\n                {milestones && milestones.map(milestone => (\n                    <div className='impact-dashboard-output-inner-container'>\n                        <h4>{milestone.Number} {milestone.Title.toLowerCase()}</h4>\n                        <MilestoneProgress milestone={milestone}/>\n                    </div>\n                ))}\n            </div>\n        )\n    }\n\n    const ManualResults =({output}) => {\n\n        const SROIs = useFirestoreSROIs(output.ID)\n\n        return (\n            <Results output={output} SROIs={SROIs}/>\n        )\n    }\n\n   const MilestoneProgress = ({milestone}) => {\n       \n        const results = useFirestoreResults(milestone.OutputID)\n\n        const goal = milestone.Number\n\n        const width = results.length*100/goal\n\n        const percentage = `${width}%`\n\n        const checkSucces = () => {\n            db.collection('Milestones')\n            .doc(milestone.docid)\n            .update({\n                Succes: true\n            })\n        }\n\n        const succes = () => {\n\n            if(results.length >= goal){\n                checkSucces()\n                return '#00cd00'\n            } else {\n                return '#63cadc'\n            }\n        }\n\n    return(\n        <div className='milestone-progress-container'>\n            <div className='percentage-container'>\n                <p>Huidig: {results.length} ({width}%)</p>\n                <p>Doel: {goal}</p>\n            </div>\n            \n            <div className='progressbar-outer-bar'>\n                <div className='progressbar-progress' style={{width: percentage, backgroundColor: succes()}}></div>\n            </div>\n\n        </div>\n        \n        )\n   }\n\n\n    const Results = ({output, SROIs}) => {\n        const [totalSROI, setTotalSROI] = useState(0)\n\n        const dataset = useFirestoreResults(output.ID)\n\n        useEffect(() => {\n            SROIs && SROIs.forEach(sroi => {\n                console.log(sroi)\n                setTotalSROI(Number(sroi.Amount))\n            })\n        })\n\n        return(\n            <>\n            <div className='internal-results-container'>\n                <ManualResultsGraph output={output}/>\n            </div>\n            <div className='internal-results-container' style={{display: SROIs.length > 0 ? 'block' : 'none'}}>\n                <div className='activity-meta-title-container'>\n                    <img src={sroiIcon} alt=\"\" />\n                    <h3>SROI</h3>\n                </div>\n                <p className='output-seeting-effect'>{dataset.length} x {totalSROI} = {dataset.length*totalSROI} per jaar</p>\n            </div>\n            </>\n        )\n    }\n\n    const QuestionnaireResults = ({instrument}) => {\n\n        const questionnaires = useFirestoreID('Questionnaires', instrument.Output.ID) \n\n        return(\n            <div>\n                {questionnaires && questionnaires.map(questionnaire => (\n                    <div className='questionnaire-results-container'>\n                        <p>Aantal responses</p>\n                        <p>{questionnaire.Responses ? questionnaire.Responses : 0}</p>\n                        <p>Bekijk analyse</p>\n                    </div>\n                ))}\n            </div>\n        )\n    }\n\n    return (\n        <div className=\"main\">\n            <LeftSideBar />\n            <LeftSideBarFullScreen/>\n            <div className=\"main-container\" style={{display: menuState}}>\n                <div className=\"page-header\">\n                    <h1>Impact dashboard</h1>\n                    <p>En overzichtelijk dashboard voor je impact management avontuur</p>\n                </div>\n                <Goals/>\n                {NoContentNotice(allGoals, 'Introduction')}\n            </div>\n        </div>\n    )\n}\n\nexport default ImpactProgress\n","/Volumes/GoogleDrive/Mijn Drive/Deccos/deccos-webapp/src/components/Impact/Stakeholders.jsx",["2015","2016"],"import {useFirestore} from \"../../firebase/useFirestore\"\nimport LeftSideBar from \"../LeftSideBar\"\nimport LeftSideBarFullScreen from \"../LeftSideBarFullScreen\"\nimport MenuStatus from \"../../hooks/MenuStatus\";\nimport arrowUpIcon from '../../images/icons/arrow-up-icon-white.png'\nimport penIcon from '../../images/icons/pen-icon-white.png'\nimport { NavLink, Link } from \"react-router-dom\";\nimport { client } from '../../hooks/Client';\nimport NoContentNotice from \"../../hooks/NoContentNotice\";\nimport ScrollToTop from \"../../hooks/ScrollToTop\";\n\nconst Stakeholders = () => {\n    const menuState = MenuStatus()\n    ScrollToTop()\n\n    const stakeholders = useFirestore('Stakeholders')\n\n    return (\n        <div className=\"main\">\n            <LeftSideBar />\n            <LeftSideBarFullScreen/>\n            <div className=\"main-container\" style={{display: menuState}}>\n                <div className=\"page-header\">\n                    <h1>Stakeholders</h1>\n                    <div className='edit-icon-header-container'>\n                        <NavLink activeClassName='active' to={`/${client}/StakeholderAnalysis`}>\n                            <img src={penIcon} alt=\"\" />\n                        </NavLink>\n                    </div>\n                </div>\n                <div className=\"card-container\">\n                    <div className='list-container list-container-stakeholder-management-page' style={{display: stakeholders.length === 0 ? 'none' : 'flex'}}>\n                    <div className='table-container'>\n                        <table>\n                            <tr>\n                                <th>CATEGORIE</th>\n                                <th>ORGANISATIE</th>\n                                <th>CONTACTPERSOON</th>\n                                <th>WIJZE VAN DIALOOH</th>\n                                <th>FREQUENTIE</th>\n                                <th>GESPREKSONDERWERP</th>\n                            </tr>\n                            {stakeholders && stakeholders.map(stakeholder => (\n                                <tr key={stakeholder.ID}>\n                                    <td>\n                                        <p>{stakeholder.Categorie}</p>\n                                    </td>\n                                    <td>\n                                        <p>{stakeholder.Organisation} </p>\n                                    </td>\n                                    <td>\n                                        <p>{stakeholder.Name}</p>\n                                    </td>\n                                    <td>\n                                        <p>{stakeholder.Dialog}</p>\n                                    </td>\n                                    <td>\n                                        <p>{stakeholder.Frequention}</p>\n                                    </td>\n                                    <td>\n                                        <p>{stakeholder.Subject}</p>\n                                    </td>\n                                </tr>\n                            ))}\n                        </table>\n                        </div>\n                    </div>\n                </div>\n                {NoContentNotice(stakeholders, 'StakeholderAnalysis')}\n            </div>\n        </div>\n    )\n}\n\nexport default Stakeholders\n","/Volumes/GoogleDrive/Mijn Drive/Deccos/deccos-webapp/src/components/Impact/QuestionnaireAnalysis.jsx",["2017"],"import LeftSideBarAuthProfile from \"../LeftSideBarAuthProfile\";\nimport LeftSideBarAuthProfileFullScreen from \"../LeftSideBarAuthProfileFullScreen\";\nimport RightSideBar from \"../rightSideBar/RightSideBar\"\nimport MenuStatus from \"../../hooks/MenuStatus\";\nimport { client } from \"../../hooks/Client\"\nimport { useHistory } from \"react-router-dom\";\nimport uuid from 'react-uuid';\nimport { db, timestamp } from \"../../firebase/config.js\"\nimport { useFirestoreID, useFirestoreQuestionnairesResponses, useFirestoreQuestionnaireFields, useFirestoreQuestionnaireAnalysis } from \"../../firebase/useFirestore\"\nimport { useState, useEffect } from 'react';\nimport Location from \"../../hooks/Location\"\nimport ButtonClicked from \"../../hooks/ButtonClicked\";\nimport ScrollToTop from \"../../hooks/ScrollToTop\";\n\nconst QuestionnaireAnalysis = () => {\n    const [questionnaireTitle, setQuestionnaireTitle] = useState('')\n\n    const menuState = MenuStatus()\n    const history = useHistory()\n    const id = uuid()\n    const route = Location()[3]\n    const options = { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric' };\n    ScrollToTop()\n\n    const questionnaires = useFirestoreID('Questionnaires', route)\n    const responses = useFirestoreQuestionnairesResponses(route)\n    const questionnaireFields = useFirestoreQuestionnaireFields(route)\n    const analysis = useFirestoreQuestionnaireAnalysis(route)\n\n    useEffect(() => {\n      questionnaires && questionnaires.forEach(questionnaire => {\n          const title = questionnaire.Title\n\n          setQuestionnaireTitle(title)\n      })\n    }, [questionnaires]);\n    \n\n    const linkRespons = (e) => {\n        const id = e.target.dataset.id \n\n        history.push(`/${client}/QuestionnairesRespons/${id}`)\n    }\n\n    const analysisArray = []\n\n    const QuestionnaireFields = ({field}) => {\n        const [analyseWord, setAnalyseWord] = useState('')\n\n        if(field.Type === 'paragraph'){\n            return (\n                <div className='question-analyse-container'>\n                    <h3>{field.Question}</h3>\n                    <p>Analyseer deze vraag op basis van een analysewoord</p>\n                    <input type=\"text\" placeholder='Type hier een analysewoord' data-id={field.ID} onChange={(e) => setAnalyseWord(e.target.value)}/>\n                    <Analysis field={field} word={analyseWord}/>\n                </div>\n            )\n        } else if (field.Type === 'scale'){\n            return(\n                <div className='question-analyse-container'>\n                    <h3>{field.Question}</h3>\n                    <p>De gemiddelde score is: </p>\n                    <ScaleScore id={field.ID} question={field.Question}/>\n                </div>\n            )\n        }\n        else {\n            return null\n        }\n    }\n\n    const ScaleScore = ({id, question}) => {\n        const scoreArray = []\n\n        responses && responses.forEach(respons => {\n            respons.Input.forEach(input => {\n                if(input.ID === id){\n                    scoreArray.push(parseInt(input.Input))\n                }\n            })\n        })\n\n        const reducer = (previousValue, currentValue) => previousValue + currentValue;\n\n        const sum = scoreArray.reduce(reducer)\n\n        const average = sum/scoreArray.length\n\n         // Set to analysis rapport\n        const analysisObject = {\n            Question: question,\n            QuestionID: id,\n            Average: average\n        }\n\n        analysisArray.push(analysisObject)\n\n         return <p>{average}</p>\n    }\n\n    const Analysis = ({field, word}) => {\n        const [inputs, setInputs] = useState([])\n\n        const analyseButton = () => {\n            const inputArray = []\n            responses && responses.forEach(respons => {\n                respons.Input.forEach(input => {\n                    if(input.ID === field.ID){\n                        if(input.Input.includes(word)){\n                            inputArray.push(input)\n                        }\n                    }\n                })\n            })\n\n            // Set to analysis rapport\n            const analysisObject = {\n                Question: field.Question,\n                QuestionID: field.ID,\n                Word: word,\n                Count: inputArray.length\n            }\n\n            analysisArray.push(analysisObject)\n\n            setInputs(inputArray)\n        }\n\n        return (\n            <div>\n                <button className='button-simple' onClick={analyseButton}>Analyseer</button>\n                <p>Dit woord komt <b>{inputs.length}</b> keer voor in de responses op deze vraag</p>\n            </div>\n        )\n    }\n\n    const saveAnalyses = (e) => {\n\n        ButtonClicked(e, 'Opgeslagen')\n\n        db.collection('QuestionnaireAnalysis')\n        .doc()\n        .set({\n            Compagny: client,\n            Timestamp: timestamp,\n            QuestionnaireID: route,\n            QuestionnaireTitle: questionnaireTitle,\n            Analysis: analysisArray\n        })\n\n    }\n\n  return  <div className=\"main\">\n        <LeftSideBarAuthProfile />\n        <LeftSideBarAuthProfileFullScreen/>\n        <div className=\"profile profile-auth-profile\" style={{display: menuState}}>\n            <div className=\"settings-inner-container\">\n                <div className=\"divider card-header\">\n                    {questionnaires && questionnaires.map(questionnaire => (\n                        <div>\n                            <h1>Analyse van {questionnaire.Title}</h1>\n                            <p>Analyseer de responses van {questionnaire.Title}</p>\n                        </div>\n                    ))}\n                </div>\n                <div className='divider'>\n                    <h2>Analyseer responses</h2>\n                        {questionnaireFields && questionnaireFields.map(field => (\n                            <QuestionnaireFields field={field}/>\n                        ))}\n                        <div>\n                            <button onClick={saveAnalyses}>Analyse opslaan</button>\n                        </div>\n                </div>\n                <div className='divider'>\n                    <h2>{analysis && 'Analyses'}</h2>\n                    {analysis && analysis.map(analyse => (\n                        <div id='questionnaire-analysis-container'>\n                            {analyse.Analysis.map(question => (\n                                <div>\n                                    <h3>{question.Question}</h3>\n                                    <p>{question.Average && `Gemiddelde score ${question.Average}`}</p>\n                                    <p>{question.Word && `Analyse woord: ${question.Word}`}</p>\n                                    <p>{question.Count && `Aantal keer in responses: ${question.Count}`}</p>\n                                </div>\n                            ))}\n                        </div>\n                    ))}\n\n                </div>\n                <div className='divider'>\n                    <h2>Responses</h2>\n                    {responses && responses.map(respons => (\n                        <div id='respons-overview-container'>\n                            <p>{respons.Timestamp.toDate().toLocaleDateString(\"nl-NL\", options)}</p>\n                            <button className='button-simple' data-is={respons.ID} onClick={linkRespons}>Bekijk</button>\n                        </div>\n                    ))}\n                </div>\n            </div>\n        </div>\n        <RightSideBar />\n        </div>\n};\n\nexport default QuestionnaireAnalysis;\n","/Volumes/GoogleDrive/mijn drive/deccos/deccos-webapp/src/index.js",[],"/Volumes/GoogleDrive/mijn drive/deccos/deccos-webapp/src/App.js",[],"/Volumes/GoogleDrive/mijn drive/deccos/deccos-webapp/src/reportWebVitals.js",[],"/Volumes/GoogleDrive/mijn drive/deccos/deccos-webapp/src/components/LoginRegister.js",["2018"],"/Volumes/GoogleDrive/mijn drive/deccos/deccos-webapp/src/components/topbar/Topbar.js",["2019","2020","2021"],"/Volumes/GoogleDrive/mijn drive/deccos/decc